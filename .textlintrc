{
  "rules": {
    "no-todo": true,
      "terminology": {
        "defaultTerms": false,
        "terms": [
          "Airbnb",
          "AppleScript",
          "AppVeyor",
          "AVA",
          "BrowserStack",
          "Browsersync",
          "Codecov",
          "CodePen",
          "CodeSandbox",
          "DefinitelyTyped",
          "EditorConfig",
          "ESLint",
          "GitHub",
          "GraphQL",
          "JavaScript",
          "JetBrains",
          "jQuery",
          "LinkedIn",
          "Lodash",
          "MacBook",
          "Markdown",
          "OpenType",
          "PayPal",
          "PhpStorm",
          "RubyMine",
          "Sass",
          "SemVer",
          "TypeScript",
          "UglifyJS",
          "WebStorm",
          "WordPress",
          "YouTube",
          ["JSDocs?", "JSDoc"],
          ["Node(?:js)?", "Node.js"],
          ["React[ .]js", "React"],
          ["SauceLabs", "Sauce Labs"],
          ["StackOverflow", "Stack Overflow"],
          ["HTTP[ /]2(?:\\.0)?", "HTTP/2"],
          ["OS X", "macOS"],
          ["Mac ?OS", "macOS"],
          ["a npm", "an npm"],

          // ECMAScript
          "ECMAScript",
          ["ES6", "ES2015"],
          ["ES7", "ES2016"],

          // Abbreviations
          "CSS",
          "HTML",
          "URL",

          // Words and phrases
          "ID", // http://stackoverflow.com/questions/1151338/id-or-id-on-user-interface
          ["id['â€™]?s", "IDs"],
          ["CLI tool(s?)", "command line tool$1"],
          ["web[- ]?site(s?)", "site$1"],
          ["repo\\b", "repository"],

          // Single word
          ["auto[- ]complete", "autocomplete"],
          ["auto[- ]format", "autoformat"],
          ["auto[- ]fix", "autofix"],
          ["auto[- ]fixing", "autofixing"],
          ["back[- ]end(\\w*)", "backend$1"],
          ["bug[- ]fix(es?)", "bugfix$1"],
          ["front[- ]end(\\w*)", "frontend$1"],
          ["hot[- ]key", "hotkey"],
          ["lock[- ]file(s?)", "lockfile$1"],
          ["name[- ]space(s?)", "namespace$1"],
          ["pre[- ]release(s?)", "prerelease$1"],

          // Multiple words
          ["change-?log(s?)", "change log$1"],
          ["css-?in-?js", "CSS in JS"],
          ["file-?type(s?)", "file type$1"],
          ["style-?guide(s?)", "style guide$1"],
          ["tree-?shaking", "tree shaking"],
          ["source-?map(s?)", "source map$1"],

          // Hyphenated
          ["client ?side", "client-side"],
          ["end ?to ?end", "end-to-end"],
          ["higher ?order", "higher-order"],
          ["server ?side", "server-side"],

          // Starts from a lower case letter in the middle of a sentence
          ["(\\w+[^.?!]\\)? )internet", "$1internet"],
          ["(\\w+[^.?!]\\)? )stylelint", "$1stylelint"]
        ]
      },
      "date-weekday-mismatch": true,
      "diacritics": true,
      "stop-words": true,
      "date-weekday-mismatch": true
  }
}
